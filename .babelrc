{
  "plugins": ["@babel/plugin-proposal-class-properties"],
  "env": {
    "development": {
      "presets": [
        ["@babel/preset-env", {
          "modules": false,
          "corejs": 3,
          "useBuiltIns": "usage",
          "loose": true,
          "targets": { "browsers": [">0.25% in US"] }
        }],
        ["@babel/preset-typescript"]
      ]
    },
    "test": {
      "presets": [
        ["@babel/preset-env", {
          "targets": { "node": "current" }
        }],
        ["@babel/preset-typescript"]
      ]
    }
  }
}

{
  "name": "task-queue.js",
  "version": "1.0.2",
  "author": "Jarid Margolin <jaridmargolin@gmail.com>",
  "description": "Extensible async task queue.",
  "homepage": "https://github.com/jaridmargolin/task-queue.js",
  "repository": "github:jaridmargolin/task-queue.js",
  "license": "MIT",
  "main": "dist/common/task-queue.js",
  "module": "dist/es/task-queue.js",
  "browser": "dist/task-queue.js",
  "scripts": {
    "build": "npm run build-lib && npm run build-docs",
    "build-docs": "jsdoc -c ./jsdoc.config.js",
    "build-lib": "rollup -c",
    "format": "prettier-eslint --write \"**/*.js\"",
    "lint": "eslint src test",
    "prepublishOnly": "npm run build",
    "publish-cov": "codecov",
    "start": "jest --watch",
    "test": "jest",
    "view-cov": "opn ./coverage/lcov-report/index.html",
    "view-docs": "opn ./docs/index.html"

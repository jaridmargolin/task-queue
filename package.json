{
  "name": "@inventory/task-queue",
  "version": "2.0.0-beta.4",
  "author": "Jarid Margolin <jaridmargolin@gmail.com>",
  "description": "Extensible async task queue.",
  "homepage": "https://github.com/jaridmargolin/task-queue",
  "repository": "github:jaridmargolin/task-queue",
  "license": "MIT",
  "main": "dist/common/task-queue.js",
  "module": "dist/es/task-queue.js",
  "browser": "dist/task-queue.js",
  "types": "dist/types/task-queue.d.ts",
  "scripts": {
    "check-types": "tsc",
    "build": "npm run build-lib && npm run build-docs && npm run build-types",
    "build-docs": "typedoc ./src/task-queue.ts",
    "build-lib": "rollup -c",
    "build-types": "tsc -p tsconfig.build.json ",
    "format": "prettier-eslint --write \"**/*.js\"",
    "lint": "eslint src",
    "prepublishOnly": "npm run build",
    "publish-cov": "codecov",
    "start": "jest --watch",
    "test": "npm run check-types && jest",
    "view-cov": "open-cli ./coverage/lcov-report/index.html",
    "view-docs": "open-cli ./docs/index.html"
  },
  "devDependencies": {
    "@babel/core": "7.7.4",
    "@babel/plugin-external-helpers": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@babel/preset-typescript": "7.7.4",
    "@types/jest": "24.0.23",
    "babel-eslint": "10.0.3",
    "codecov": "4.0.0-0",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.0",
    "husky": "1.3.1",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "opn-cli": "5.0.0",
    "prettier-eslint-cli": "4.7.1",
    "rollup": "1.27.5",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-terser": "5.1.2",
    "typedoc": "0.15.3",
    "typescript": "3.5.3"
  }
}
